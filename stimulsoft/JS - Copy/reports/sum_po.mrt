        <Columns isList="true" count="3">
          <value>project_name,System.String</value>
          <value>SUM_x0028_po_item.poi_amount_x0029_,System.Decimal</value>
          <value>po_remark,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>total_amount</Name>
        <NameInSource>MySQL</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT
	project.project_name,
	SUM(po_item.poi_amount),
	po.po_remark
	FROM
	po
	INNER JOIN po_item ON po.po_pono = po_item.poi_ref
INNER JOIN project ON po.po_project = project.project_id
GROUP BY project_name
ORDER BY SUM(po_item.poi_amount) DESC</SqlCommand>
      </total_amount>
