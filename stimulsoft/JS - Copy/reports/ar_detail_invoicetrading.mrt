        <SqlCommand>SELECT inv_no,inv_project,ar_invdown_header.compcode,inv_receipt,inv_receiptamt,inv_contactamount,SUM(inv_downamt) as invamt,inv_owner FROM ar_invdown_header JOIN ar_invdown_detail on ar_invdown_detail.inv_ref = ar_invdown_header.inv_no WHERE inv_status != "D" GROUP BY inv_no,ar_invdown_header.inv_period
UNION ALL 
SELECT inv_no,inv_project,ar_invprogress_header.compcode,inv_receipt,inv_receiptamt,inv_contactamount,SUM(inv_progressamt) as invamt,inv_owner FROM ar_invprogress_header JOIN ar_invprogress_detail on ar_invprogress_detail.inv_ref = ar_invprogress_header.inv_no WHERE inv_status != "D" GROUP BY inv_no,ar_invprogress_header.inv_period
UNION ALL
SELECT inv_no,inv_project,ar_invretention_header.compcode,inv_receipt,inv_receiptamt,inv_contactamount,SUM(inv_retentionamt) as invamt,inv_owner FROM ar_invretention_header JOIN ar_invretention_detail on ar_invretention_detail.inv_ref = ar_invretention_header.inv_no WHERE inv_status != "D" GROUP BY inv_no,ar_invretention_header.inv_period</SqlCommand>
      </down>
