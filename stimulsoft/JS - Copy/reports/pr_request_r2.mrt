        <Columns isList="true" count="9">
          <value>approve,System.String</value>
          <value>docno,System.String</value>
          <value>type,System.String</value>
          <value>DATE_FORMAT_x0028_docdate_x002C__x0027__x0025_d_x002F__x0025_m_x002F__x0025_Y_x0027__x0029_,System.String</value>
          <value>member,System.String</value>
          <value>project,System.String</value>
          <value>sum_x0028_pettycashi_unitprice_x0029_,System.Double</value>
          <value>department,System.Int32</value>
          <value>advname,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>DataSource2</Name>
        <NameInSource>Connection1</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT approve,docno,"PC" as type,DATE_FORMAT(docdate,'%d/%m/%Y'),member,advname,project,sum(pettycashi_unitprice),department
FROM pettycash 
INNER JOIN pettycash_item ON pettycash.docno = pettycash_item.pettycashi_ref 
WHERE approve LIKE '%{st}%'
{get_doc}
{get_date}
and project LIKE '%{proid}%' 
and advname LIKE '%{butn}%'
GROUP BY pettycash.docno
ORDER BY pettycash.docno ASC</SqlCommand>
      </DataSource2>
