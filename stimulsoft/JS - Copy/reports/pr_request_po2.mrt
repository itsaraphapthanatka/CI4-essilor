          <value>sum_x0028_poi_amount_x0029_,System.Double</value>
          <value>pr_department,System.Int32</value>
          <value>po_pono,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>DataSource1</Name>
        <NameInSource>Connection1</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT pr_approve,pr_prno,"PR" as type,DATE_FORMAT(pr_prdate,'%d/%m/%Y'),pr_reqname,pr_project,sum(poi_amount),pr_department,po_pono
FROM pr 
INNER JOIN po ON pr.pr_prno = po.po_prno
INNER JOIN po_item ON po.po_pono = po_item.poi_ref
WHERE pr_approve LIKE '%{st}%' 
{docs}
{get_date}
and pr_project LIKE '%{proid}%' and pr_reqname LIKE '%{buyn}%'
GROUP BY po.po_pono
UNION ALL 
SELECT approve,docno,"PC" as type,DATE_FORMAT(docdate,'%d/%m/%Y'),advname,project,sum(pettycashi_unitprice),department,"" as po_pono 
FROM pettycash 
INNER JOIN pettycash_item ON pettycash.docno = pettycash_item.pettycashi_ref 
WHERE approve LIKE '%{st}%' 
{doce}
{get_date2} 
and project LIKE '%{proid}%' 
and advname LIKE '%{buyn}%'
GROUP BY pettycash.docno</SqlCommand>
      </DataSource1>
