UNION ALL
SELECT ap_date,doc_date,ap_period,ap_year,acct_no,amtcr,amtdr,ap_no,ap_pettycash_header.compcode FROM ap_pettycash_header JOIN ap_gl on ap_pettycash_header.ap_no =ap_gl.vchno WHERE ap_status !=  "delete" AND ap_gl.doctype = ap_pettycash_header.ap_type and ap_period &gt;= {period} and  ap_period &lt;= {period1}
UNION ALL
SELECT apv_date,apv_duedate,apv_glperiod,apv_glyear,acct_no,amtcr,amtdr,apv_code,apv_header.compcode FROM apv_header JOIN ap_gl on apv_header.apv_code = ap_gl.vchno AND ap_gl.doctype = apv_header.apv_type WHERE apv_status != "delete" and apv_glperiod &gt;= {period} and  apv_glperiod &lt;= {period1}
UNION ALL
SELECT aps_invdate,aps_duedate,aps_period,aps_year,acct_no,amtcr,amtdr,aps_code,aps_header.compcode FROM aps_header JOIN ap_gl on aps_header.aps_code = ap_gl.vchno AND ap_gl.doctype = aps_header.aps_type WHERE aps_status != "delete" and aps_period &gt;= {period} and  aps_period &lt;= {period1}
UNION ALL
