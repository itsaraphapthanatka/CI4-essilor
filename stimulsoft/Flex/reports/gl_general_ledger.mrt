        <SqlCommand>SELECT acc_ardate,acc_due,acc_period,acc_year,acc_codeac,acc_cr,acc_dr,acc_no,ar_account_header.compcode FROM ar_account_header JOIN ar_account_detail ON ar_account_detail.acc_ref = ar_account_header.acc_no WHERE acc_status != "D" and acc_period &gt;= {period} and  acc_period &lt;= {period1}
