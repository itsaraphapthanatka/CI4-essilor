JOIN ic_proj_warehouse ON ic_proj_warehouse.whcode = stockcard.warehouse and ic_proj_warehouse.project_id = stockcard.stock_project

WHERE stockcard.stock_matcode LIKE '%%'
and stockcard.stock_type= 'issue'


{get_pro_id}
{get_wh}
{get_mat}
{get_date_af}

GROUP BY stockcard.stock_matcode,warehouse
ORDER BY stockcard.stock_matname DESC</SqlCommand>
