SELECT ar_invdown_header.compcode,inv_no,sum(inv_downamt) as amt, inv_system,inv_date,inv_remark,inv_receiptamt,inv_project,inv_type,inv_owner,inv_contactamount FROM
ar_invdown_header JOIN ar_invdown_detail ON ar_invdown_header.inv_no = ar_invdown_detail.inv_ref AND ar_invdown_detail.inv_period = ar_invdown_header.inv_period GROUP BY inv_no,ar_invdown_header.inv_period
UNION ALL
