        <Name>apgl</Name>
        <NameInSource>MySQL</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT
project.project_code,
project.project_name,
system.systemname,
system.systemcode,
ap_gl.costcode,
ap_gl.matname,
ap_gl.amtdr,
ap_gl.amtcr
FROM
ap_gl
INNER JOIN project ON ap_gl.projectid = project.project_id
INNER JOIN system ON ap_gl.systemcode = system.systemcode
WHERE project.project_code LIKE '%%'
{whereyear}
{whereperiod}
{wheresystem}
{whereidpro}
ORDER BY ap_gl.vchno ASC
</SqlCommand>
