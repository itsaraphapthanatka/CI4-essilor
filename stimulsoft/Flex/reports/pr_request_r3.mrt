          <value>pr_reqname,System.String</value>
          <value>pr_project,System.String</value>
          <value>sum_x0028_pri_amount_x0029_,System.Double</value>
          <value>pr_department,System.Int32</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1"/>
        <Name>DataSource1</Name>
        <NameInSource>Connection1</NameInSource>
        <Parameters isList="true" count="0"/>
        <SqlCommand>SELECT pr_approve,pr_prno,"PR" as type,DATE_FORMAT(pr_prdate,'%d/%m/%Y'),pr_reqname,pr_project,sum(pri_amount),pr_department
FROM pr 
INNER JOIN pr_item ON pr.pr_prno = pr_item.pri_ref
WHERE pr_approve LIKE '%{st}%' 

and pr_project LIKE '%{proid}%' and pr_reqname LIKE '%{buyn}%'
GROUP BY pr.pr_prno
UNION ALL 
SELECT approve,docno,"PC" as type,DATE_FORMAT(docdate,'%d/%m/%Y'),advname,project,sum(pettycashi_unitprice),department
FROM pettycash 
INNER JOIN pettycash_item ON pettycash.docno = pettycash_item.pettycashi_ref 
WHERE approve LIKE '%{st}%' 

and project LIKE '%{proid}%' 
and advname LIKE '%{buyn}%'
GROUP BY pettycash.docno</SqlCommand>
      </DataSource1>
