        <Parameters isList="true" count="0"/>
        <SqlCommand>SELECT ap_year,ap_period,ap_type,SUM(amtcr) as amtcr,SUM(amtdr) as amtdr,acct_no,ap_code,ap_project,ap_vendor,compcode
FROM    (SELECT ap_project,ap_vendor,ap_pettycash_header.compcode as compcode,ap_year,ap_period,ap_type,SUM(amtcr) as amtcr,SUM(amtdr) as amtdr,acct_no as acct_no,ap_no as ap_code FROM ap_pettycash_header JOIN ap_gl on ap_pettycash_header.ap_no =ap_gl.vchno AND ap_gl.doctype = ap_pettycash_header.ap_type GROUP BY ap_vendor,acct_no
					UNION ALL
          SELECT apv_project as ap_project,apv_vender as ap_vendor,apv_header.compcode,apv_glyear as ap_year,apv_glperiod as ap_period,apv_type as ap_type,SUM(amtcr) as amtcr,SUM(amtdr) as amtdr,acct_no,apv_code as ap_code FROM apv_header JOIN ap_gl on apv_header.apv_code = ap_gl.vchno AND ap_gl.doctype = apv_header.apv_type GROUP BY ap_vendor,acct_no
					UNION ALL
					SELECT aps_project as ap_project,aps_vender as ap_vendor,aps_header.compcode,aps_year as ap_year,aps_period as ap_period,aps_type as ap_type,SUM(amtcr) as amtcr,SUM(amtdr) as amtdr,acct_no,aps_code as ap_code FROM aps_header JOIN ap_gl on aps_header.aps_code = ap_gl.vchno AND ap_gl.doctype = aps_header.aps_type GROUP BY ap_vendor,acct_no
					UNION ALL	
					SELECT apd_project as ap_project,apd_vender as ap_vendor,ap_down_header.compcode,apd_year as ap_year,apd_period as ap_period,apd_type as ap_type,SUM(amtcr) as amtcr,SUM(amtdr) as amtdr,acct_no,apd_code as ap_code FROM ap_down_header JOIN ap_gl on ap_down_header.apd_code = ap_gl.vchno AND ap_gl.doctype = ap_down_header.apd_type GROUP BY ap_vendor,acct_no
					UNION ALL
					SELECT apr_project as ap_project,apr_vender as ap_vendor,ap_ret_header.compcode,apr_year as ap_year,apr_period as ap_period,apr_type as ap_type,SUM(amtcr) as amtcr,SUM(amtdr) as amtdr,acct_no,apr_code as ap_code FROM ap_ret_header JOIN ap_gl on ap_ret_header.apr_code = ap_gl.vchno AND ap_gl.doctype = ap_ret_header.apr_type GROUP BY ap_vendor,acct_no
        ) t
GROUP BY ap_vendor,acct_no
ORDER BY ap_vendor,acct_no</SqlCommand>
      </DataSource1>
