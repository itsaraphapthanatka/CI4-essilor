	DISTINCT
	whcode, 
	whname,
	stockcard.stock_matname, 
	stockcard.stock_matcode, 
	stockcard.stock_type,
	stockcard.stock_date, 
	stockcard.stock_priceunit,

	IF(stockcard.stock_type = 'receive' or stockcard.stock_type = 'keyin' ,  Sum(stockcard.stock_qty), 0) AS 'IN', 
