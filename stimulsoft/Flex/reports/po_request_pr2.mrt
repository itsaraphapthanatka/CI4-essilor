        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1"/>
        <Name>WO Report</Name>
        <NameInSource>Connection1</NameInSource>
        <Parameters count="0" isList="true"/>
        <SqlCommand>SELECT po_approve,po_pono,"PO" as type,DATE_FORMAT(po_podate,'%d/%m/%Y'),po_prname,po_project,sum(poi_amount) as poi_amount,po_prno,po_department
FROM po 
INNER JOIN po_item ON po.po_pono = po_item.poi_ref
where po_approve LIKE '%{st}%' 
{docs}
{get_date}
and po_project LIKE '%{proid}%' 
and po_prname LIKE '%{buyn}%'
GROUP BY po.po_pono
UNION ALL
SELECT lo.status,lo_lono,"WO" as type,DATE_FORMAT(lodate,'%d/%m/%Y'),lo.user,projectcode,sum(lo_priceunit) as lo_priceunit,"" as po_prno,department
FROM lo 
INNER JOIN lo_detail ON lo.lo_lono = lo_detail.lo_ref 
WHERE lo.status LIKE '%{st}%'
{doce}
{get_date2}
and projectcode LIKE '%{proid}%' 
and lo.user LIKE '%{buyn}%'</SqlCommand>
      </WO_x0020_Report>
